{
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "bundler",
        "types": ["bun-types"],
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "./dist",
        "rootDir": "./src",
        "jsx": "react",
        "paths": {
            "@paowa-bot/core": ["./src/core/index.ts"],
            "@paowa-bot/core/*": ["./src/core/*"]
        }
    }
}
